
<header>
	<div>
		<h2 class="text-center">Here are your results!</h2>
	</div>
</header>

<div class="container-fluid col-xs-10 col-xs-offset-1 text-center">
{{#each pets}}
<div class="pet col-xs-10 col-xs-offset-1 col-sm-3 text-center">
<!-- 	<p>Pic: {{media.photos}}</p> -->
	<p>Name: {{name}}</p>
	<p>Age: {{age}}</p>
	<p>Size: {{size}}</p>
	<p>Sex: {{sex}}</p>
	<p>Breed: {{breeds}}</p>
	<p>{{contact.city}}</p>
	<p>{{contact.phone}}</p>
	<p>{{contact.email}}</p>

</div>
{{/each}}
</div>
